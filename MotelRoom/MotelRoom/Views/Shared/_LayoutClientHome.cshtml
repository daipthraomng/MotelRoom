<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta content="" name="keywords">
    <meta content="" name="description">
    <link href="~/Template/img/favicon.png" rel="icon">
    <link href="~/Template/img/apple-touch-icon.png" rel="apple-touch-icon">
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet">

    <!-- Bootstrap CSS File -->
    <link href="~/Template/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    @RenderSection("Styles", false)
    <!-- Libraries CSS Files -->
    <link href="~/Template/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Template/lib/animate/animate.min.css" rel="stylesheet">
    <link href="~/Template/lib/ionicons/css/ionicons.min.css" rel="stylesheet">
    <link href="~/Template/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">

    <!-- Main Stylesheet File -->
    <link href="~/Template/css/style.css" rel="stylesheet">
    <title>@ViewData["Title"] - MotelRoom</title>

    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css" rel="stylesheet"> @*to use awesome icon*@

</head>
<body>
    <div class="click-closed"></div>

    <!--/ Nav Star /-->
    <nav class="navbar navbar-default navbar-trans navbar-expand-lg mb-3 ">
        <div class="container">
            <button class="navbar-toggler collapsed" type="button" data-toggle="collapse" data-target="#navbarDefault"
                    aria-controls="navbarDefault" aria-expanded="false" aria-label="Toggle navigation">
                <span></span>
                <span></span>
                <span></span>
            </button>
            <a class="navbar-brand text-brand" asp-area="" asp-controller="Home" asp-action="Index">Motel<span class="color-b">Room</span></a>
            <button type="button" class="btn btn-link nav-search navbar-toggle-box-collapse d-md-none" data-toggle="collapse"
                    data-target="#navbarTogglerDemo01" aria-expanded="false">
                <span class="fa fa-search" aria-hidden="true"></span>
            </button>
            <div class="navbar-collapse collapse justify-content-center" id="navbarDefault">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="ClientFavouriteMotel">Danh sách yêu thích</a>
                    </li>
                    <li class="nav-item">
                        <partial name="_LoginPartial.cshtml" />
                    </li>
                </ul>
            </div>
            <button type="button" class="btn btn-b-n navbar-toggle-box-collapse d-none d-md-block" data-toggle="collapse"
                    data-target="#navbarTogglerDemo01" aria-expanded="false">
                <span class="fa fa-search" aria-hidden="true"></span>
            </button>
        </div>
    </nav>
    <!--/ Nav End /-->

    <div class="container" style="margin-bottom: 70px">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer>
        <div class="container">
            <p class="copyright color-text-a">
                &copy; Copyright
                <span class="color-a">MotelRoom</span> All Rights Reserved.
            </p>
        </div>
    </footer>
    <!--/ Footer End /-->

    <a href="#" class="back-to-top"><i class="fa fa-chevron-up"></i></a>
    <div id="preloader"></div>


    <script src="~/Template/lib/jquery/jquery.min.js"></script>
    <script src="~/Template/lib/jquery/jquery-migrate.min.js"></script>
    <script src="~/Template/lib/popper/popper.min.js"></script>
    <script src="~/Template/lib/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Template/lib/easing/easing.min.js"></script>
    <script src="~/Template/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/Template/lib/scrollreveal/scrollreveal.min.js"></script>

    <!-- Template Main Javascript File -->
    <script src="~/Template/js/main.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
